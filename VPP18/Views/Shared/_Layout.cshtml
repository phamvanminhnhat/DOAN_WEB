    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <title>Văn Phòng Phẩm EIGHT-TEEN</title>

    <link href="~/Content/images/favicon.ico" rel="shortcut icon" type="image/x-icon">

    <!-- jQuery -->
    <script src="~/Scripts/js/jquery-2.0.0.min.js" type="text/javascript"></script>

    <!-- Bootstrap4 files-->
    <script src="~/Scripts/js/bootstrap.bundle.min.js" type="text/javascript"></script>
    <link href="~/Content/css/bootstrap.css" rel="stylesheet" type="text/css" />

    <!-- Font awesome 5 -->
    <link href="fonts/fontawesome/~/Content/css/all.min.css" type="text/css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css" integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g==" crossorigin="anonymous" referrerpolicy="no-referrer" />

    <!-- custom style -->
    <link href="~/Content/css/ui.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/responsive.css" rel="stylesheet" type="text/css" />

    <!-- custom javascript -->


</head>
<body>

    <header class="section-header">
        <section class="header-main border-bottom">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-xl-2 col-lg-3 col-md-12">
                        <a href="/Home/Index" class="brand-wrap">
                            <img style="width:100px"class="logo" src="~/Content/images/logo.jpg">
                        </a> <!-- brand-wrap.// -->
                    </div>
                    <div class="col-xl-6 col-lg-5 col-md-6">
                        <form action="#" class="search-header">
                            <div class="input-group w-100">

                                <input type="text" class="form-control" placeholder="Search">

                                <div class="input-group-append">
                                    <button class="btn btn-primary" type="submit">
                                        <i class="fa fa-search"></i> Search
                                    </button>
                                </div>
                            </div>
                        </form> <!-- search-wrap .end// -->
                    </div> <!-- col.// -->
                    <div class="col-xl-4 col-lg-4 col-md-6">
                        <div class="widgets-wrap float-md-right">

                            <div class="widget-header mr-3">
                                <a href="#" class="widget-view">
                                    <div class="icon-area">
                                        <i class="fa fa-comment-dots"></i>
                                        <span class="notify">1</span>
                                    </div>
                                    <small class="text"> Thông báo </small>
                                </a>
                            </div>
                            <div class="widget-header mr-3">
                                <a href="/GioHang/GioHang" class="widget-view">
                                    <div class="icon-area">
                                        <i class="fa-solid fa-basket-shopping"></i>
                                    </div>

                                    <small class="text"> Giỏ hàng </small>
                                </a>
                            </div>

                        </div> <!-- widgets-wrap.// -->
                    </div> <!-- col.// -->
                </div> <!-- row.// -->
            </div> <!-- container.// -->
        </section> <!-- header-main .// -->


        <nav class="navbar navbar-main navbar-expand-lg border-bottom">
            <div class="container">

                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#main_nav" aria-controls="main_nav" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="main_nav">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link" href="/Home/Index">Trang chủ</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/Home/About">Giới thiệu</a>
                        </li>

                        <li class="nav-item dropdown">
                            <a class="nav-link " href="/SanPham/ListSP"> SẢN PHẨM </a>
                            
                        </li>

                        <li class="nav-item">
                            <a class="nav-link" href="/Home/Contact">Liên hệ</a>
                        </li>


                    </ul>
                    <ul class="navbar-nav ml-md-auto">

                        <li class="nav-item dropdown">
                            @if (Session["TenND"] == null)
                            {
                        <a class="nav-link dropdown-toggle" href="#" data-toggle="dropdown">Tài khoản</a>
                                        <div class="dropdown-menu dropdown-menu-right">
                                            <a class="dropdown-item" href="/Home/DangKy">Đăng kí</a>
                                            <a class="dropdown-item" href="/Home/Dangnhap">Đăng nhập</a>

                                        </div> }
                                    else
                                    {
                        <a class="nav-link dropdown-toggle" href="#" data-toggle="dropdown">@(((VPP18.Models.NGUOIDUNG)Session["TenND"]).TenND)</a>
                                        <div class="dropdown-menu dropdown-menu-right">
                                            <a class="dropdown-item" href="/Home/DangXuat">Đăng Xuất</a>

                                        </div>}

                        </li>
                    </ul>
                </div> <!-- collapse .// -->
            </div> <!-- container .// -->
        </nav>
    </header> <!-- section-header.// -->
    @RenderBody()


    <!-- container end.// -->
    <!-- ========================= SECTION SUBSCRIBE  ========================= -->
    <!-- ========================= SECTION SUBSCRIBE END// ========================= -->
