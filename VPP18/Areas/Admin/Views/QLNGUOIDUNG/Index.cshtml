@model IEnumerable<VPP18.Models.NGUOIDUNG>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}



<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdND)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TenND)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SDT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DiaChi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MatKhau)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.IdND)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TenND)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.SDT)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.DiaChi)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Email)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.MatKhau)
    </td>
    <td>
        <a class="btn btn-sm btn-primary" href="@Url.Action("Edit", "QLNGUOIDUNG", new { id = item.IdND })"><i class="fas fa-edit"></i> Edit</a>
        <a class="btn btn-sm btn-primary" href="@Url.Action("Detail", "QLNGUOIDUNG", new { id = item.IdND})"><i class="fas fa-info"></i> Detail</a>
        <a class="btn btn-sm btn-primary" href="@Url.Action("Delete", "QLNGUOIDUNG", new { id = item.IdND})"><i class="fas fa-trash-alt"></i> Delete</a>
    </td>
</tr>
}

</table>
